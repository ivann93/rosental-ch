  {
    "name": "Typography",
    "settings": [
      {
        "type": "font_picker",
        "id": "headings_font",
        "label": "Headings",
        "default": "montserrat_n5"
      },
      {
        "type": "range",
        "id": "headings_size",
        "label": "Base size",
        "min": 18,
        "max": 32,
        "step": 1,
        "unit": "px",
        "default": 24
      },
      {
        "type": "range",
        "id": "headings_size_alt",
        "label": "Large headings",
        "min": 18,
        "max": 52,
        "step": 1,
        "unit": "px",
        "default": 32,
        "info": "Affects large titles from the slider, rich text and image with text sections."
      },
      {
        "type": "font_picker",
        "id": "body_font",
        "label": "Body",
        "default": "montserrat_n4"
      },
      {
        "type": "range",
        "id": "body_size",
        "label": "Base size",
        "min": 12,
        "max": 20,
        "step": 1,
        "unit": "px",
        "default": 14
      },
      {
        "type": "range",
        "id": "body_size_alt",
        "label": "Primary navigation",
        "min": 12,
        "max": 24,
        "step": 1,
        "unit": "px",
        "default": 17
      },
      {
        "type": "font_picker",
        "id": "typography_quotes_new",
        "label": "Quotes",
        "default": "times_new_roman_i4"
      }
    ]
  },
  {
    "name": "Product grid",
    "settings": [
      {
        "type": "select",
        "id": "grid_aspect",
        "label": "Media aspect ratio",
        "options": [
          {
            "value": "natural",
            "label": "Natural",
            "group": "Uncropped"
          },
          {
            "value": "1",
            "label": "Square (1:1)",
            "group": "Cropped"
          },
          {
            "value": "0.666667",
            "label": "Tall (2:3)",
            "group": "Cropped"
          },
          {
            "value": "1.33333",
            "label": "Short (4:3)",
            "group": "Cropped"
          }
        ],
        "default": "natural",
        "info": "When using the natural aspect ratio, make sure that you resize your thumbnails at the same size, for a clean grid design. Using one of the cropped setting, all thumbnails will be resized to the same dimension."
      },
      {
        "type": "checkbox",
        "id": "grid_show_secondary_image",
        "label": "Show second product media on hover"
      },
      {
        "type": "checkbox",
        "id": "grid_enhance_featured_product",
        "label": "Emphasize featured products",
        "info": "[Learn more](https://shopify-support.krownthemes.com/article/471-general-settings#featured-products)"
      },
      {
        "type": "select",
        "id": "grid_discount",
        "label": "Show discount as...",
        "options": [
          {
            "value": "badge",
            "label": "Text"
          },
          {
            "value": "percentage",
            "label": "Percentage"
          }
        ],
        "default": "badge"
      },
      {
        "type": "select",
        "id": "grid_style",
        "label": "Caption placement",
        "options": [
          {
            "value": "overlay",
            "label": "Overlay",
            "group": "Visible on roll over"
          },
          {
            "value": "classic",
            "label": "Below image",
            "group": "Always visible"
          }
        ],
        "default": "classic"
      },
      {
        "type": "color",
        "id": "grid_color_bg",
        "label": "Overlay caption background",
        "default": "#000000"
      },
      {
        "type": "color",
        "id": "grid_color_text",
        "label": "Overlay caption text color",
        "default": "#ffffff"
      },
      {
        "type": "header",
        "content": "Product reviews",
        "info": "You need to install [Shopify's free Product Reviews app](https://apps.shopify.com/product-reviews) before enabling this option."
      },
      {
        "type": "checkbox",
        "id": "show_reviews_badge",
        "label": "Show reviews badge",
        "default": false
      }
    ]
  },
  {
    "name": "Klevu",
    "settings": [
      {
        "type": "text",
        "id": "klevu_api_key",
        "label": "Klevu api key",
        "info": "The api key is mandatory, if left out, all klevu features will be disabled"
      },
      {
        "type": "header",
        "content": "Klevu Search"
      },
      {
        "type": "paragraph",
        "content": "Klevu search is enabled as soon as the api key is provided."
      },
      {
        "type": "paragraph",
        "content": "Note: Predictive search needs to be disabled in order for klevu search to function properly. (Uncheck the 'Enable predictive search' in the search settings)."
      },
      {
        "type": "header",
        "content": "Klevu product recommendations"
      },
      {
        "type": "text",
        "id": "klevu_product_recs_snippet",
        "label": "Klevu Recommendations HTML snippet",
        "info": "The snippet is mandatory, if left out, klevu recommendations will be disabled"
      }
