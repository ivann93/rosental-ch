<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="preload" as="style" href="{{ 'theme.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'main.css' | asset_url }}">
    <link rel="preload" as="script" href="{{ 'main.js' | asset_url }}">

    <link rel="preconnect" href="https://cdn.shopify.com">
    <link rel="preconnect" href="https://fonts.shopifycdn.com">
    <link rel="dns-prefetch" href="https://productreviews.shopifycdn.com">
    <link rel="dns-prefetch" href="//code.jquery.com/jquery-3.5.1.slim.min.js">

    <script src="//code.jquery.com/jquery-3.5.1.slim.min.js" 
      integrity="sha256-4+XzXVhsDmqanXGHaHvgh1gMQKX40OUvDEBTu8JcmNs="
      crossorigin="anonymous">
    </script>
    {% if shop.permanent_domain == 'rosental-de.myshopify.com' %}
		  {% include "gtm-head" %}
      {% include "hotjar" %}
    {% endif %}

    {{ 'option_selection.js' | shopify_asset_url | script_tag }}

    <link rel="stylesheet" href="{{ 'theme.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'main.css' | asset_url }}">

    {{ 'main.js' | asset_url | script_tag }}
    {% render 'header-scripts.zipifypages', renderctx: 'tpl' %}{{ content_for_header }}
  </head>
  <body>
    {% if shop.permanent_domain == 'rosental-de.myshopify.com' %}
		  {% include "gtm-body" %}
    {% endif %}
    {% render 'layout-cart-drawer' %}
    {% include 'page-content.zipifypages', renderctx: 'tpl' %}
    {% render 'page-footer.zipifypages', renderctx: 'tpl' %}
    {% render 'layout-cart-init' %}
  </body>
</html>{% layout none %}